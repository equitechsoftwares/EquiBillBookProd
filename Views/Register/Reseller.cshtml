
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="@ViewBag.BusinessSetting.Favicon" type="image/x-icon">
    <title>@(ViewBag.PageTitle ?? ViewBag.BusinessSetting.BusinessName + " | Reseller Register")</title>
    
    <!-- SEO Meta Tags -->
    <meta name="description" content="@(ViewBag.MetaDescription ?? "Join EquiBillBook's reseller program and earn 25% commission on GST billing software sales. Start your own software business with our proven billing solution.")">
    <meta name="keywords" content="@(ViewBag.MetaKeywords ?? "EquiBillBook reseller, GST billing software reseller, software reseller program")">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:title" content="@(ViewBag.OgTitle ?? "Become a Reseller - EquiBillBook GST Billing Software")">
    <meta property="og:description" content="@(ViewBag.OgDescription ?? "Join our reseller program and earn 25% commission on GST billing software sales. White-label options available.")">
    <meta property="og:type" content="website">
    <meta property="og:url" content="@(ViewBag.CanonicalUrl ?? "https://equibillbook.com/register/reseller")">
    <meta property="og:image" content="@(ViewBag.OgImage ?? "https://equibillbook.com/Content/web-assets/images/innerbanner.png")">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="@(ViewBag.TwitterTitle ?? "EquiBillBook Reseller Program - Earn 25% Commission")">
    <meta name="twitter:description" content="@(ViewBag.TwitterDescription ?? "Join EquiBillBook's reseller program. Earn 25% commission on GST billing software sales.")">
    <meta name="twitter:image" content="@(ViewBag.TwitterImage ?? "https://equibillbook.com/Content/web-assets/images/innerbanner.png")">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="@(ViewBag.CanonicalUrl ?? "https://equibillbook.com/register/reseller")">
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/plugins/fontawesome-free/css/all.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="~/Content/plugins/select2/css/select2.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/Content/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/assets/css/adminlte.min.css">
    <!-- Toastr -->
    <link rel="stylesheet" href="~/Content/plugins/toastr/toastr.min.css">
    <link href="~/Content/assets/css/admin.css" rel="stylesheet" />
    @*<style>
            .select2-container--default .select2-selection--single .select2-selection__rendered {
                color: #a19ba2 !important;
            }
        </style>*@
    <style>
        .goog-logo-link {
            display: none !important;
        }

        .goog-te-gadget {
            color: transparent !important;
            font-size: 0 !important;
        }

            .goog-te-gadget .goog-te-combo {
                /*padding: 0.375rem 0.75rem;*/
                padding: 0.575rem 0.75rem;
                /*color: #495057;*/
                border: 1px solid #ced4da;
                border-radius: 0.25rem;
            }

            .goog-te-gadget .goog-te-combo {
                margin: 0 !important;
            }

        body > .skiptranslate {
            display: none;
        }

        .skiptranslate span {
            display: none;
        }

        body {
            top: 0 !important
        }

        #goog-gt-tt, .goog-te-balloon-frame {
            display: none !important;
        }

        .goog-text-highlight {
            background: none !important;
            box-shadow: none !important;
        }

        .VIpgJd-ZVi9od-aZ2wEe-wOHMyf-ti6hGc {
            display: none;
        }

        #goog-gt- {
            display: none !important;
        } 

    </style>
</head>
<body class="hold-transition login-page">
    <div class="login-box" style="margin-top:10%;">
        <div class="login-logo">
            <a href="@ViewBag.BusinessSetting.ParentWebsiteUrl"><img src="@ViewBag.BusinessSetting.BusinessLogo" style=" width: 222px; display: block; margin-left: auto; margin-right: auto;" /></a>
        </div>
        <!-- /.login-logo -->
        <div class="card card-outline card-primary">
            <div id="google_translate_element" style="margin-left:auto;padding:20px 20px 0 0;"></div>
            <div class="card-body login-card-body divRegister">
                @*<img src="~/Content/web-assets/images/logo2.png" style=" width: 200px; display: block; margin-left: auto; margin-right: auto;" />*@
                @*<br />*@
                <h4 class="text-center">Reseller Register</h4>
                <p class="login-box-msg danger">It might take upto 3 days to approve your account</p>
                <div class="alert alert-success" id="divErrorMsg" style="display:none">
                    @*<button type="button" class="close" onclick="closeErrorMsg()">×</button>*@
                    <h5><i class="icon fas fa-check"></i> Submitted successfully!</h5>
                    Your application is successfully submitted. Please wait until our backend team verifies & approves your account
                    @*<hr />
        <div class="form-group">
            <label style="width:100%">A Record</label>
            <p class="mb-0"> Current IP Address: <span id="divCurrentIp" class="font-weight-bold"> </span> </p>
            <p> Required IP Address: <span class="font-weight-bold"> 108.181.191.157 </span><a href="javascript:void(0)" class="btn btn-primary text-decoration-none" onclick="copyCode('103.171.180.90')">Copy</a></p>
        </div>*@
                </div>
                <div>
                    <div class="form-group">
                        <div class="input-group" id="">
                            <input id="txtUsername" type="text" class="form-control" placeholder="User Id" onchange="checkUserName()" />
                            <div class="input-group-append">
                                <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                    @*<i class="fa fa-calendar"></i>*@
                                </div>
                            </div>
                        </div>
                        <small class="text-red font-weight-bold errorText" id="divUsername"></small>
                    </div>
                    <div class="input-group mb-3">
                        <input type="text" class="form-control" placeholder="Name" id="txtName">
                        <div class="input-group-append">
                            <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                @*<span class="fas fa-user"></span>*@
                            </div>
                        </div>
                        @*<div class="input-group-append ml-0">
                                <div class="input-group-text" style="border-left: 1px solid #ced4da;">
                                    <span class="fas fa-user"></span>
                                </div>
                            </div>*@
                    </div>

                    <div class="input-group mb-3">
                        <input type="email" class="form-control" placeholder="Email Id" id="txtEmailId">
                        <div class="input-group-append ">
                            <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                @*<span class="fas fa-envelope"></span>*@
                            </div>
                        </div>
                    </div>
                    <div class="input-group mb-3">
                        <select class="form-control select2" id="ddlCountry" onchange="fetchDialingCode()">
                            @*<select class="form-control select2" id="ddlCountry">*@
                            <option value="0" selected disabled>Select Country</option>
                            @foreach (var item in ViewBag.Countrys)
                            {
                                @*<option value="@item.CountryId">@item.Country - @item.CurrencyName (@item.CurrencyCode) </option>*@
                                <option value="@item.CountryId">@item.Country </option>
                            }
                        </select>
                        @*<div class="input-group-append">
                                <div class="input-group-text">
                                    <span class="fas fa-flag"></span>
                                </div>
                            </div>*@
                    </div>
                    <div class="mb-3">
                        <!-- text input -->
                        <div class="form-group">
                            <div class="input-group">
                                @*<span class="input-group-append divDialingCode" style="display:none">
                                        <a href="javascript:void(0)" class="btn bg-gray-light lblDialingCode"> +xx </a>
                                    </span>*@
                                <input type="text" class="form-control" placeholder="Mobile No" id="txtMobileNo" onkeypress="return onlyNumberKey(event)" maxlength="10">
                                <div class="input-group-append ">
                                    <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                        @*<span class="fas fa-phone"></span>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="input-group mb-3 divCurrency" style="display:none">

                    </div>

                    <div class="input-group mb-3 divTimeZone" style="display:none">

                    </div>

                    <div class="input-group mb-3">
                        <input type="password" class="form-control" placeholder="Password" id="txtPassword">
                        <div class="input-group-append ">
                            <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                @*<span class="fas fa-lock"></span>*@
                            </div>
                        </div>
                    </div>
                    <div class="input-group mb-3">
                        <input type="password" class="form-control" placeholder="Confirm Password" id="txtCPassword">
                        <div class="input-group-append ">
                            <div class="input-group-text p-0" style="border-left: 1px solid #ced4da;">
                                @*<span class="fas fa-lock"></span>*@
                            </div>
                        </div>
                    </div>
                    <div class="input-group">
                        <div class="custom-file" style="width:100%">
                            @*<input type="file" class="custom-file-input" id="Image">*@
                            <input type="file" id="IdProof" class="custom-file-input" accept='.png,.jpg,.jpeg' onchange="getProfileBase64()">
                            <label class="custom-file-label" for="Logo">Upload a Valid Id Proof</label>
                        </div>
                        <img id="blah" alt="" height="60" style="padding-top:10px;">
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <div class="icheck-primary">
                                <input type="checkbox" id="chkTerms_Conditions">
                                <label for="chkTerms_Conditions">
                                    I accept the <a href="@ViewBag.BusinessSetting.ResellerTermsUrl" target="_blank" class="">Terms & Conditions</a>
                                </label>
                            </div>
                        </div>
                        <div class="col-8">
                            @*<a href="/resellerterms" class="">Go Back</a>*@
                        </div>
                        <!-- /.col -->
                        <div class="col-4">
                            <a href="javascript:void(0)" class="btn btn-primary btn-block" onclick="RegisterOtp()">Register</a>
                        </div>
                        <!-- /.col -->
                    </div>
                    @*<p class="mb-0">
                            <a href="/login" class="" onclick="login()">I already have a membership</a>
                        </p>*@
                </div>

                <!-- /.social-auth-links -->
                @*<p class="mb-1">
                        <a href="/login">Login In</a>
                    </p>*@

            </div>
            <div class="card-body login-card-body divOtp hidden">
                @*<img src="~/Content/assets/img/logo.png" style=" width: 200px; display: block; margin-left: auto; margin-right: auto;" />
                    <br />*@
                <p class="login-box-msg lblOtp"></p>

                <div>
                    <div class="input-group mb-3">
                        <input type="text" class="form-control" placeholder="Otp" id="txtOtp">
                        <div class="input-group-append">
                            <div class="input-group-text">
                                <span class="fas fa-key"></span>
                            </div>
                        </div>
                    </div>
                    <label id="timer" style="font-size:1.2rem;text-align:center;display:block">
                    </label>
                    <p class="login-box-msg text-danger">Make sure to check your spam folder</p>
                    <div class="mt-2 divResend" style="text-align:center;display:none" id="">
                        <a href="javascript:void(0)" onclick="RegisterOtp()" style=" color: #e25b5c;">Resend one time password</a>
                    </div>
                    <div class="row">

                        <div class="col-8">
                            <a href="javascript:void(0)" class="" onclick="previous()">Entered a wrong Email Id?</a>
                        </div>
                        <!-- /.col -->
                        <div class="col-4">
                            <a href="javascript:void(0)" class="btn btn-primary btn-block" onclick="register()">Submit</a>
                        </div>
                        <!-- /.col -->
                    </div>
                </div>

                <!-- /.social-auth-links -->
                @*<p class="mb-1">
                        <a href="/login">Login In</a>
                    </p>*@

            </div>
            <!-- /.login-card-body -->
        </div>
    </div>

    <!-- /.login-box -->

    <div id="divLoading" style="margin: 0px; padding: 0px; position: fixed; right: 0px; top: 0px; width: 100%; height: 100%; z-index: 30001; opacity: 0.8;display:none">
        <div class="overlay-wrapper">
            <div class="overlay"><i class="fas fa-3x fa-sync-alt fa-spin"></i><div class="text-bold pt-2">Loading...</div></div>
        </div>
    </div>

    <audio id="success"><source src="~/Content/Audio/success.mp3"></audio>
    <audio id="error"><source src="~/Content/Audio/error.mp3"></audio>

    <!-- jQuery -->
    <script src="~/Content/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- SweetAlert2 -->
    <script src="~/Content/plugins/sweetalert2/sweetalert2.min.js"></script>
    <!-- Toastr -->
    <script src="~/Content/plugins/toastr/toastr.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/assets/js/adminlte.min.js"></script>
    <!-- Select2 -->
    <script src="~/Content/plugins/select2/js/select2.full.min.js"></script>
    <script src="~/Content/JQuery/Customer/Common/Common.js"></script>
    <script src="~/Content/JQuery/Customer/Authentication/ResellerRegister.js"></script>

    @*<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/js-cookie/3.0.1/js.cookie.min.js"></script>

    <script type="text/javascript">
        var googtrans = Cookies.get('googtrans');
        if (!googtrans) {
            Cookies.set('googtrans', '/auto/en');
        }
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({ pageLanguage: '' }, 'google_translate_element');
        }
    </script>
</body>
</html>
