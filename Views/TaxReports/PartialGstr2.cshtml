                <td>@ViewBag.AdjustmentOfAdvancesTotalTaxableValue</td>
                <td>@ViewBag.AdjustmentOfAdvancesTotalCGST</td>
                <td>@ViewBag.AdjustmentOfAdvancesTotalSGST</td>
                <td>@ViewBag.AdjustmentOfAdvancesTotalIGST</td>
                <td>@ViewBag.AdjustmentOfAdvancesTotalCESS</td>
            </tr>
