

@{
    <table id="tblData" class="table table-bordered ">
        <thead id="thead">
            <tr>
                <th>Document Details </th>
                <th>No. of Record</th>
                <th>Total Invoice Value (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>Total Taxable Value (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>CGST (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>SGST/ UTGST (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>IGST (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>CESS (@ViewBag.BusinessSetting.CurrencySymbol)</th>
            </tr>
        </thead>
        <tbody class="table-body">
            <tr>
                <td>Inward supplies received from registered person<br /> including reverse charge supplies-3, 4A </td>
                <td>@ViewBag.B2BTotalCount</td>
                <td>@ViewBag.B2BTotalInvoiceValue</td>
                <td>@ViewBag.B2BTotalTaxableValue</td>
                <td>@ViewBag.B2BTotalCGST</td>
                <td>@ViewBag.B2BTotalSGST</td>
                <td>@ViewBag.B2BTotalIGST</td>
                <td>@ViewBag.B2BTotalCESS</td>
            </tr>
            <tr>
                <td>Import of Inputs/ Capital Goods and Supplies<br /> received from SEZ-5</td>
                <td>@ViewBag.B2CLargeTotalCount</td>
                <td>@ViewBag.B2CLargeTotalInvoiceValue</td>
                <td>@ViewBag.B2CLargeTotalTaxableValue</td>
                <td>@ViewBag.B2CLargeTotalCGST</td>
                <td>@ViewBag.B2CLargeTotalSGST</td>
                <td>@ViewBag.B2CLargeTotalIGST</td>
                <td>@ViewBag.B2CLargeTotalCESS</td>
            </tr>
            <tr>
                <td>Import of Service-4C </td>
