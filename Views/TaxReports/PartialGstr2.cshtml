                <td>@ViewBag.HsnWiseSummaryTotalCGST</td>
                <td>@ViewBag.HsnWiseSummaryTotalSGST</td>
                <td>@ViewBag.HsnWiseSummaryTotalIGST</td>
                <td>@ViewBag.HsnWiseSummaryTotalCESS</td>
            </tr>
