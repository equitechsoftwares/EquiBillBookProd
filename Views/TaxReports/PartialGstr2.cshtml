                <td>@ViewBag.ExportsTotalInvoiceValue</td>
                <td>@ViewBag.ExportsTotalTaxableValue</td>
                <td>@ViewBag.ExportsTotalCGST</td>
                <td>@ViewBag.ExportsTotalSGST</td>
                <td>@ViewBag.ExportsTotalIGST</td>
                <td>@ViewBag.ExportsTotalCESS</td>
            </tr>
            <tr>
                <td>Inward supplies from an unregistered supplier-4B </td>
