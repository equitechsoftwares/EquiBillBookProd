                <td>@ViewBag.B2CSmallTotalInvoiceValue</td>
                <td>@ViewBag.B2CSmallTotalTaxableValue</td>
                <td>@ViewBag.B2CSmallTotalCGST</td>
                <td>@ViewBag.B2CSmallTotalSGST</td>
                <td>@ViewBag.B2CSmallTotalIGST</td>
                <td>@ViewBag.B2CSmallTotalCESS</td>
            </tr>
            <tr>
