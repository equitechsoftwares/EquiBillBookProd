                <td>@ViewBag.CreditDebitUnRegisteredTotalInvoiceValue</td>
                <td>@ViewBag.CreditDebitUnRegisteredTotalTaxableValue</td>
                <td>@ViewBag.CreditDebitUnRegisteredTotalCGST</td>
                <td>@ViewBag.CreditDebitUnRegisteredTotalSGST</td>
                <td>@ViewBag.CreditDebitUnRegisteredTotalIGST</td>
                <td>@ViewBag.CreditDebitUnRegisteredTotalCESS</td>
