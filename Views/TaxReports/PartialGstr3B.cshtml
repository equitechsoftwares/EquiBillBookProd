    <br />
    <div>
        <h6>3.2 Of the supplies shown in 3.1 (a) above, details of inter-State supplies made to unregistered persons, composition taxable persons and UIN holders</h6>
    </div>
    <table id="tblData" class="table table-bordered ">
        <thead id="thead">
            <tr>
                <th> </th>
                <th>Place Of Supply</th>
                <th>Taxable Value (@ViewBag.BusinessSetting.CurrencySymbol)</th>
                <th>Integrated Tax (@ViewBag.BusinessSetting.CurrencySymbol)</th>
            </tr>
        </thead>
        <tbody class="table-body">
            <tr class="bg-secondary">
                <td>1 </td>
                <td>2</td>
                <td>3</td>
                <td>4</td>
            </tr>
            <tr>
                <td colspan="4">
                    Supplies made to Unregistered Persons
                </td>
            </tr>
            @foreach (var item in ViewBag.InterStateUnregistered)
            {
                <tr onclick="window.location.href='/taxreports/Gstr3BDetails?title=Supplies made to Unregistered Persons&PlaceOfSupply=@item.PlaceOfSupply';" style="cursor: pointer;">
                    <td></td>
                    <td>@item.PlaceOfSupply</td>
                    <td>
                        @(String.Format("{0:0.##}", item.AmountExcTax))
                    </td>
                    <td>
                        @(String.Format("{0:0.##}", item.TotalIgstValue))
                    </td>
                </tr>
            }
            <tr>
                <td colspan="4">
                    Supplies made to Composition Taxable Persons
                </td>
            </tr>
            @foreach (var item in ViewBag.InterStateComposition)
            {
                <tr onclick="window.location.href='/taxreports/Gstr3BDetails?title=Supplies made to Composition Taxable Persons&PlaceOfSupply=@item.PlaceOfSupply';" style="cursor: pointer;">
                    >
                    <td></td>
                    <td>@item.PlaceOfSupply</td>
                    <td>
                        @(String.Format("{0:0.##}", item.AmountExcTax))
                    </td>
                    <td>
                        @(String.Format("{0:0.##}", item.TotalIgstValue))
                    </td>
                </tr>
            }
            <tr>
                <td colspan="4">
                    Supplies made to UIN holders
                </td>
            </tr>
            <tr>
                <td colspan="4" class="text-center">We are not tracking supplies made to UIN holders</td>
            </tr>
        </tbody>
    </table>

