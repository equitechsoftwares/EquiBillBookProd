                <td>Sub-total (I to L above)</td>
                <td>@(ViewBag.Gstr9_4ITotalTaxableValue+ ViewBag.Gstr9_4JTotalTaxableValue)</td>
                <td>@(ViewBag.Gstr9_4ITotalCGST+ ViewBag.Gstr9_4JTotalCGST)</td>
                <td>@(ViewBag.Gstr9_4ITotalSGST + ViewBag.Gstr9_4JTotalSGST)</td>
                <td>@(ViewBag.Gstr9_4ITotalIGST+ ViewBag.Gstr9_4JTotalIGST)</td>
                <td>@(ViewBag.Gstr9_4ITotalCESS+ ViewBag.Gstr9_4JTotalCESS)</td>
            </tr>
            <tr style="background:#eee">
                <td>N</td>
                <td>Supplies and advances on which tax is to be paid (H + M) above</td>
                <td>
                    @(ViewBag.Gstr9_4ATotalTaxableValue + ViewBag.Gstr9_4BTotalTaxableValue + ViewBag.Gstr9_4CTotalTaxableValue +
ViewBag.Gstr9_4DTotalTaxableValue+ ViewBag.Gstr9_4FTotalTaxableValue+ ViewBag.Gstr9_4GTotalTaxableValue+
ViewBag.Gstr9_4ITotalTaxableValue + ViewBag.Gstr9_4JTotalTaxableValue)
                </td>
                <td>
                    @(ViewBag.Gstr9_4ATotalCGST + ViewBag.Gstr9_4BTotalCGST+ ViewBag.Gstr9_4CTotalCGST+
ViewBag.Gstr9_4DTotalCGST+ ViewBag.Gstr9_4FTotalCGST+ ViewBag.Gstr9_4GTotalCGST+
ViewBag.Gstr9_4ITotalCGST + ViewBag.Gstr9_4JTotalCGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_4ATotalSGST+ ViewBag.Gstr9_4BTotalSGST+ ViewBag.Gstr9_4CTotalSGST+
ViewBag.Gstr9_4DTotalSGST+ ViewBag.Gstr9_4FTotalSGST+ ViewBag.Gstr9_4GTotalSGST+
ViewBag.Gstr9_4ITotalSGST + ViewBag.Gstr9_4JTotalSGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_4ATotalIGST+ ViewBag.Gstr9_4BTotalIGST+ ViewBag.Gstr9_4CTotalIGST+
ViewBag.Gstr9_4DTotalIGST+ ViewBag.Gstr9_4FTotalIGST+ ViewBag.Gstr9_4GTotalIGST+
ViewBag.Gstr9_4ITotalIGST + ViewBag.Gstr9_4JTotalIGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_4ATotalCESS+ ViewBag.Gstr9_4BTotalCESS+ ViewBag.Gstr9_4CTotalCESS+
ViewBag.Gstr9_4DTotalCESS+ ViewBag.Gstr9_4FTotalCESS+ ViewBag.Gstr9_4GTotalCESS+
ViewBag.Gstr9_4ITotalCESS + ViewBag.Gstr9_4JTotalCESS)
                </td>
            </tr>
            <tr class="bg-secondary">
                <td>5</td>
                <td colspan="6" class="text-center">Details of Outward supplies on which tax is not payable as declared in returns filed during the financial year</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5A';" style="cursor: pointer;">
                <td> A</td>
                <td>Zero rated supply (Export) without payment of tax</td>
                <td>@ViewBag.Gstr9_5ATotalTaxableValue</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5B';" style="cursor: pointer;">
                <td> B</td>
                <td>Supply to SEZs without payment of tax</td>
                <td>@ViewBag.Gstr9_5BTotalTaxableValue</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5C';" style="cursor: pointer;">
                <td> C</td>
                <td>Supplies on which tax is to be paid by the recipient on<br /> reverse charge basis</td>
                <td>@ViewBag.Gstr9_5CTotalTaxableValue</td>
                <td>@ViewBag.Gstr9_5CTotalCGST</td>
                <td>@ViewBag.Gstr9_5CTotalSGST</td>
                <td>@ViewBag.Gstr9_5CTotalIGST</td>
                <td>@ViewBag.Gstr9_5CTotalCESS</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5D';" style="cursor: pointer;">
                <td> D</td>
                <td>Exempted</td>
                <td>@ViewBag.Gstr9_5DTotalTaxableValue</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5E';" style="cursor: pointer;">
                <td> E</td>
                <td>Nil Rated</td>
                <td>@ViewBag.Gstr9_5ETotalTaxableValue</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5F';" style="cursor: pointer;">
                <td> F</td>
                <td>Non-GST supply	</td>
                <td>@ViewBag.Gstr9_5FTotalTaxableValue</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr style="background:#eee">
                <td> G</td>
                <td>Sub-total (A to F above)</td>
                <td>
                    @(ViewBag.Gstr9_5ATotalTaxableValue + ViewBag.Gstr9_5BTotalTaxableValue + ViewBag.Gstr9_5CTotalTaxableValue +
ViewBag.Gstr9_5DTotalTaxableValue + ViewBag.Gstr9_5ETotalTaxableValue + ViewBag.Gstr9_5FTotalTaxableValue)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalCGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalSGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalIGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalCESS)
                </td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5H';" style="cursor: pointer;">
                <td> H</td>
                <td>Credit Notes issued in respect of transactions specified<br /> in A to F above (-)</td>
                <td>@ViewBag.Gstr9_5HTotalTaxableValue</td>
                <td>@ViewBag.Gstr9_5HTotalCGST</td>
                <td>@ViewBag.Gstr9_5HTotalSGST</td>
                <td>@ViewBag.Gstr9_5HTotalIGST</td>
                <td>@ViewBag.Gstr9_5HTotalCESS</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=5I';" style="cursor: pointer;">
                <td>I</td>
                <td>Debit Notes issued in respect of transactions specified<br /> in A to F above (+)</td>
                <td>@ViewBag.Gstr9_5ITotalTaxableValue</td>
                <td>@ViewBag.Gstr9_5ITotalCGST</td>
                <td>@ViewBag.Gstr9_5ITotalSGST</td>
                <td>@ViewBag.Gstr9_5ITotalIGST</td>
                <td>@ViewBag.Gstr9_5ITotalCESS</td>
            </tr>
            <tr>
                <td>J</td>
                <td>Supplies / tax declared through Amendments (+)</td>
                <td colspan="5" class="text-center">- - - - Not Supported - - - -</td>
            </tr>
            <tr>
                <td>K</td>
                <td>Supplies / tax reduced through Amendments (-)</td>
                <td colspan="5" class="text-center">- - - - Not Supported - - - -</td>
            </tr>
            <tr style="background:#eee">
                <td>L</td>
                <td>Sub-Total (H to K above)</td>
                <td>@(ViewBag.Gstr9_5HTotalTaxableValue+ ViewBag.Gstr9_5ITotalTaxableValue)</td>
                <td>@(ViewBag.Gstr9_5HTotalCGST+ ViewBag.Gstr9_5ITotalCGST)</td>
                <td>@(ViewBag.Gstr9_5HTotalSGST+ ViewBag.Gstr9_5ITotalSGST)</td>
                <td>@(ViewBag.Gstr9_5HTotalIGST+ ViewBag.Gstr9_5ITotalIGST)</td>
                <td>@(ViewBag.Gstr9_5HTotalCESS+ ViewBag.Gstr9_5ITotalCESS)</td>
            </tr>
            <tr style="background:#eee">
                <td>M</td>
                <td>Turnover onwhich tax is not to be paid (G + L above)</td>
                <td>
                    @(ViewBag.Gstr9_5ATotalTaxableValue + ViewBag.Gstr9_5BTotalTaxableValue + ViewBag.Gstr9_5CTotalTaxableValue +
ViewBag.Gstr9_5DTotalTaxableValue + ViewBag.Gstr9_5ETotalTaxableValue + ViewBag.Gstr9_5FTotalTaxableValue+
ViewBag.Gstr9_5HTotalTaxableValue + ViewBag.Gstr9_5ITotalTaxableValue)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalCGST+ ViewBag.Gstr9_5HTotalCGST + ViewBag.Gstr9_5ITotalCGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalSGST+ ViewBag.Gstr9_5HTotalSGST + ViewBag.Gstr9_5ITotalSGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalIGST+ ViewBag.Gstr9_5HTotalIGST + ViewBag.Gstr9_5ITotalIGST)
                </td>
                <td>
                    @(ViewBag.Gstr9_5CTotalCESS+ ViewBag.Gstr9_5HTotalCESS + ViewBag.Gstr9_5ITotalCESS)
                </td>
            </tr>
            <tr style="background:#eee">
                <td>N</td>
                <td>Total Turnover (including advances) (4N + 5M -4G above)</td>
                <td>
                    @(((ViewBag.Gstr9_4ATotalTaxableValue + ViewBag.Gstr9_4BTotalTaxableValue + ViewBag.Gstr9_4CTotalTaxableValue +
ViewBag.Gstr9_4DTotalTaxableValue+ ViewBag.Gstr9_4FTotalTaxableValue+ ViewBag.Gstr9_4GTotalTaxableValue+
ViewBag.Gstr9_4ITotalTaxableValue + ViewBag.Gstr9_4JTotalTaxableValue) +
(ViewBag.Gstr9_5ATotalTaxableValue + ViewBag.Gstr9_5BTotalTaxableValue + ViewBag.Gstr9_5CTotalTaxableValue +
ViewBag.Gstr9_5DTotalTaxableValue + ViewBag.Gstr9_5ETotalTaxableValue + ViewBag.Gstr9_5FTotalTaxableValue +
ViewBag.Gstr9_5HTotalTaxableValue + ViewBag.Gstr9_5ITotalTaxableValue))- ViewBag.Gstr9_4GTotalTaxableValue)
