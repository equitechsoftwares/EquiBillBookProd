                <td>@ViewBag.Gstr9_4ATotalIGST</td>
                <td>@ViewBag.Gstr9_4ATotalCESS</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=4B';" style="cursor: pointer;">
