                <td>
                    @(((ViewBag.Gstr9_4ATotalCGST + ViewBag.Gstr9_4BTotalCGST+ ViewBag.Gstr9_4CTotalCGST+
ViewBag.Gstr9_4DTotalCGST+ ViewBag.Gstr9_4FTotalCGST+ ViewBag.Gstr9_4GTotalCGST+
ViewBag.Gstr9_4ITotalCGST + ViewBag.Gstr9_4JTotalCGST) +
(ViewBag.Gstr9_5CTotalCGST + ViewBag.Gstr9_5HTotalCGST + ViewBag.Gstr9_5ITotalCGST)
)- ViewBag.Gstr9_4GTotalCGST)
                </td>
                <td>
                    @(((ViewBag.Gstr9_4ATotalSGST+ ViewBag.Gstr9_4BTotalSGST+ ViewBag.Gstr9_4CTotalSGST+
ViewBag.Gstr9_4DTotalSGST+ ViewBag.Gstr9_4FTotalSGST+ ViewBag.Gstr9_4GTotalSGST+
ViewBag.Gstr9_4ITotalSGST + ViewBag.Gstr9_4JTotalSGST) +
(ViewBag.Gstr9_5CTotalSGST + ViewBag.Gstr9_5HTotalSGST + ViewBag.Gstr9_5ITotalSGST))- ViewBag.Gstr9_4GTotalSGST)
                </td>
                <td>
                    @(((ViewBag.Gstr9_4ATotalIGST+ ViewBag.Gstr9_4BTotalIGST+ ViewBag.Gstr9_4CTotalIGST+
ViewBag.Gstr9_4DTotalIGST+ ViewBag.Gstr9_4FTotalIGST+ ViewBag.Gstr9_4GTotalIGST+
ViewBag.Gstr9_4ITotalIGST + ViewBag.Gstr9_4JTotalIGST) +
(ViewBag.Gstr9_5CTotalIGST + ViewBag.Gstr9_5HTotalIGST + ViewBag.Gstr9_5ITotalIGST))- ViewBag.Gstr9_4GTotalIGST)
                </td>
                <td>
                    @(((ViewBag.Gstr9_4ATotalCESS+ ViewBag.Gstr9_4BTotalCESS+ ViewBag.Gstr9_4CTotalCESS+
ViewBag.Gstr9_4DTotalCESS+ ViewBag.Gstr9_4FTotalCESS+ ViewBag.Gstr9_4GTotalCESS+
ViewBag.Gstr9_4ITotalCESS + ViewBag.Gstr9_4JTotalCESS) +
(ViewBag.Gstr9_5CTotalCESS + ViewBag.Gstr9_5HTotalCESS + ViewBag.Gstr9_5ITotalCESS))- ViewBag.Gstr9_4GTotalCESS)
                </td>
            </tr>
        </tbody>
    </table>

    <table id="tblData" class="table table-bordered ">
        <thead id="thead">
            <tr class="bg-secondary">
                <th>Pt. III  </th>
                <th colspan="6" class="text-center">Details of ITC as declared in returns filed during the financial year</th>
            </tr>
        </thead>
        <tbody class="table-body">
            <tr>
                <td rowspan="3"> </td>
            </tr>
            <tr>
                <td> Description</td>
                <td>Type</td>
                <td>Central Tax</td>
                <td>State Tax / UT Tax</td>
                <td>Integrated Tax</td>
                <td>Cess</td>
            </tr>
            <tr>
                <td> 1</td>
                <td>2</td>
                <td>3</td>
                <td>4</td>
                <td>5</td>
                <td>6</td>
            </tr>
            <tr class="bg-secondary">
                <td>6</td>
                <td colspan="6">Details of ITC availed as declared in returns filed during the financial year</td>
            </tr>
            <tr>
                <td> A</td>
                <td colspan="2">Total amount of input tax credit availed through FORM GSTR-3B (sum<br /> total of Table 4A of FORM GSTR-3B)</td>
                <td colspan="4" class="text-center">- - - - Not Supported - - - -</td>
            </tr>
            <tr>
                <td rowspan="4"> B</td>
                <td rowspan="4">
                    Inward supplies (other than imports and inward supplies<br />
                    liable to reverse charge but includes services received<br />
                    from SEZs)
                </td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=6BInputs';" style="cursor: pointer;">
                <td>Inputs</td>
                <td>@ViewBag.Gstr9_6BInputsTotalCGST</td>
                <td>@ViewBag.Gstr9_6BInputsTotalSGST</td>
                <td>@ViewBag.Gstr9_6BInputsTotalIGST</td>
                <td>@ViewBag.Gstr9_6BInputsTotalCESS</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=6BCapitalGoods';" style="cursor: pointer;">
                <td>Capital Goods</td>
                <td>@ViewBag.Gstr9_6BCapitalGoodsTotalCGST</td>
                <td>@ViewBag.Gstr9_6BCapitalGoodsTotalSGST</td>
                <td>@ViewBag.Gstr9_6BCapitalGoodsTotalIGST</td>
                <td>@ViewBag.Gstr9_6BCapitalGoodsTotalCESS</td>
