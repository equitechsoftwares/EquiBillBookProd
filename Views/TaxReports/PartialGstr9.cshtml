                <td>Inward supplies on which tax is to be paid on reverse<br /> charge basis</td>
                <td>@ViewBag.Gstr9_4GTotalTaxableValue</td>
                <td>@ViewBag.Gstr9_4GTotalCGST</td>
                <td>@ViewBag.Gstr9_4GTotalSGST</td>
                <td>@ViewBag.Gstr9_4GTotalIGST</td>
                <td>@ViewBag.Gstr9_4GTotalCESS</td>
