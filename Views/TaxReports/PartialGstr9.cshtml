                <td>@ViewBag.Gstr9_4FTotalIGST</td>
                <td>@ViewBag.Gstr9_4FTotalCESS</td>
            </tr>
            <tr onclick="window.location.href='/taxreports/Gstr9Details?Type=4G';" style="cursor: pointer;">
