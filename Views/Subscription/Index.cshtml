                                        <th>Payment Date </th>
                                        <th>Invoice# </th>
                                        <th>Amount</th>
                                        <th class="hide noPrint">Action</th>
                                    </tr>
                                </thead>
                                @{
                                    var count = 1;
                                    <tbody class="table-body">
                                        @foreach (var item in ViewBag.MyPlan.UnusedPlans)
                                        {
                                            <tr>
                                                <td>@(count)</td>
                                                <td>@item.PaidOn.ToString(Request.Cookies["BusinessSetting"].Value.Split('&')[0].Split('=')[1] + " " + Request.Cookies["BusinessSetting"].Value.Split('&')[1].Split('=')[1])</td>
